<%= form_for("customer", url: customers_path, method: :post ) do |f| %>
  <%= f.text_field(:first_name, placeholder: "Enter First Name" ) %><br><br>
    <%= f.text_field(:last_name, placeholder: "Enter Last Name" ) %><br><br>
      <%= f.text_field(:contact, placeholder: "Enter Contact" ) %><br><br>
        <%= f.date_field(:birth_date, placeholder: "Enter Birthdate" ) %><br><br>
          <%= f.text_field(:address, placeholder: "Enter Address" ) %><br><br>
            <%= f.submit "Submit" %>
              <% end %>